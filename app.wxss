/**app.wxss**/
@import 'pages/font/font.wxss';

@font-face {
  font-family: 'HiraginoSansGB-W3';
  src:url('http://112.109.215.18:8101/wechatWX/font/HiraginoSansGBW3_0.otf');
} 
 view,
text {
  font-family: 'HiraginoSansGB-W3';
}  
page {
  background: #f2f2f2;
}

view {
  box-sizing: border-box;
  height: auto;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
image {
  width: 100%;
  height: auto;
}
.floatL {
  float: left;
}
.floatR {
  float: right;
}
.floatL::after
.floatR::after,{
  display: block;
  content: '';
  clear: both
}




