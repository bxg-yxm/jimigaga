/* pages/loginBefore/loginBefore.wxss */
page{
  background: #fff;
  border-top: 1px solid #f2f2f2;
}
.containerW{
  padding: 0 30rpx;
  font-size: 28rpx;
}
.userInfo{
  padding: 40rpx 0 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #000;
}
.userInfo image{
  display: block;
  width: 160rpx;
  margin: 0 auto 20rpx;
  border-radius: 50%;
}

.hello{
  color: #666;
  margin-bottom: 20rpx;
}
.gaga{
  color: #666;
  margin-top: 50rpx;
  margin-bottom: 30rpx;
  font-size: 28rpx;
}
button{
  height: 84rpx;
  border-radius: 10rpx;
  color: #eb5e1b;
  text-align: center;
  line-height: 84rpx;
  background: #fff;
  padding: 0;
  margin: 0 auto;
  font-size: 30rpx;
  border: 1px solid #f2f2f2;
}
button::after{
  border: none;
}
.frist{
  background: #eb5e1b;
  color: #fff;
  border: none;
}





















